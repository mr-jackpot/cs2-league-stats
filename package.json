{
  "name": "cs2-league-stats",
  "version": "1.0.0",
  "description": "Counter-Strike 2 league statistics API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "cs2",
    "counter-strike",
    "stats",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/router": "^15.2.0",
    "dotenv": "^16.4.1",
    "koa": "^3.1.1",
    "koa-bodyparser": "^4.4.1",
    "koa-helmet": "^9.0.0",
    "koa-ratelimit": "^6.0.0",
    "node-cache": "^5.1.2",
    "yaml": "^2.8.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/koa": "^3.0.1",
    "@types/koa__cors": "^5.0.1",
    "@types/koa__router": "^12.0.5",
    "@types/koa-bodyparser": "^4.3.13",
    "@types/koa-ratelimit": "^5.0.5",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "supertest": "^7.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
